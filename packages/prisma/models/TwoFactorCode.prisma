model TwoFactorCode {
  id        String   @id @default(uuid())
  code      String
  userId    String   // Rimosso @unique per permettere pi√π codici per utente
  user      User     @relation(fields: [userId], references: [id])
  createdAt DateTime @default(now())
  expiresAt DateTime
  
  @@index([userId])
  @@map("TwoFactorCode")
}